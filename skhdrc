ctrl - f [
    "Brave Browser" : curl -X POST "http://localhost:$MLREDZONE_PORT/toggle-fullscreen" &>/dev/null
]

ctrl - up [
    "Brave Browser" : yabai -m window --focus north
]
ctrl - down [
    "Brave Browser" : yabai -m window --focus south
]
ctrl - right [
    "Brave Browser" : yabai -m window --focus east
]
ctrl - left [
    "Brave Browser" : yabai -m window --focus west
]
